{
  "name": "connect-postgres",
  "version": "1.0.0",
  "description": "example connect integration which puts urban airship data into postgres",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: there are no tests\" && exit 1",
    "user": "createuser -h $PGHOST -p $PGPORT -D -R urban_airship_connect",
    "schema": "cat ./schema.sql && psql --dbname urban_airship_connect --host $PGHOST --port $PGPORT -U $PGUSER --file schema.sql",
    "connect": "psql --dbname urban_airship_connect --host $PGHOST --port $PGPORT -U $PGUSER",
    "start": "node index.js | pino",
    "init": "npm run schema && npm run start",
    "format": "standard --fix"
  },
  "author": "Urban Airship",
  "license": "MIT",
  "dependencies": {
    "concat-stream": "^1.6.0",
    "pg": "^6.1.5",
    "pino": "^4.3.0",
    "through2": "^2.0.3",
    "urban-airship-connect": "^1.1.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "standard": "^10.0.2"
  }
}
